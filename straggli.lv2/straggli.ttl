@prefix doap:  <http://usefulinc.com/ns/doap#> .
@prefix foaf:  <http://xmlns.com/foaf/0.1/> .
@prefix lv2:   <http://lv2plug.in/ns/lv2core#> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix units: <http://lv2plug.in/ns/extensions/units#> .

<https://github.com/ecashin>
        a foaf:Person ;
        foaf:name "Ed Cashin" ;
        foaf:mbox <mailto:ed.cashin@acm.org> ;
        foaf:homepage <http://noserose.net/> .

<https://github.com/ecashin/straggli>
        a lv2:Plugin ,
                lv2:AmplifierPlugin ;
lv2:project <https://github.com/ecashin/straggli> ;
doap:name "Straggli" ;
doap:license <https://opensource.org/licenses/BSD-3-Clause> ;
        lv2:optionalFeature lv2:hardRTCapable ;
        lv2:port [
                a lv2:InputPort ,
                lv2:ControlPort ;
                lv2:index 0 ;
                lv2:symbol "gain" ;
                lv2:name "Gain" ;
                lv2:default 0.0 ;
                lv2:minimum -90.0 ;
                lv2:maximum 24.0 ;
                units:unit units:db ;
                lv2:scalePoint [
                        rdfs:label "+5" ;
                        rdf:value 5.0
                ] , [
                        rdfs:label "0" ;
                        rdf:value 0.0
                ] , [
                        rdfs:label "-5" ;
                        rdf:value -5.0
                ] , [
                        rdfs:label "-10" ;
                        rdf:value -10.0
                ]
        ] , [
                a lv2:AudioPort ,
                     lv2:InputPort ;
                lv2:index 1 ;
                lv2:symbol "input_left" ;
                lv2:name "InputLeft"
        ] , [
                a lv2:AudioPort ,
                     lv2:InputPort ;
                lv2:index 2 ;
                lv2:symbol "input_right" ;
                lv2:name "InputRight"
        ] , [
                a lv2:AudioPort ,
                     lv2:OutputPort ;
                lv2:index 3 ;
                lv2:symbol "output_left" ;
                lv2:name "OutputLeft"
        ] , [
                a lv2:AudioPort ,
                     lv2:OutputPort ;
                lv2:index 4 ;
                lv2:symbol "output_right" ;
                lv2:name "OutputRight"
        ] , [
                a lv2:InputPort ,
                lv2:ControlPort ;
                lv2:index 5 ;
                lv2:symbol "max_delay_ms" ;
                lv2:name "MaxDelayMS" ;
                lv2:default 200.0 ;
                lv2:minimum 1.0 ;
                lv2:maximum 1999.0
        ] , [
                a lv2:InputPort ,
                lv2:ControlPort ;
                lv2:index 6 ;
                lv2:symbol "max_abs_acc" ;
                lv2:name "MaxAbsAcc" ;
                lv2:default 0.3 ;
                lv2:minimum 0.0001 ;
                lv2:maximum 1.0
        ] , [
                a lv2:InputPort ,
                lv2:ControlPort ;
                lv2:index 7 ;
                lv2:symbol "max_abs_velo" ;
                lv2:name "MaxAbsVelo" ;
                lv2:default 0.8 ;
                lv2:minimum 0.0001 ;
                lv2:maximum 50.0
        ] , [
                a lv2:InputPort ,
                lv2:ControlPort ;
                lv2:index 8 ;
                lv2:symbol "wet_mix" ;
                lv2:name "WetMix" ;
                lv2:default 90.0 ;
                lv2:minimum 0.0 ;
                lv2:maximum 100.0
        ] ;
        rdfs:comment "stereo randomized delay lines"
        .